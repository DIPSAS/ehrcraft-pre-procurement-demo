                </term_definitions>
                <term_definitions code="at0002">
                    <items id="text">Tidpunkt</items>
                    <items id="description">Den specifika tidpunkten när bedömningen är gjord.</items>
                </term_definitions>
                <term_definitions code="at0003">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0004">
                    <items id="text">Systoliskt blodtryck</items>
                    <items id="description">Intervall för uppmätt systoliskt blodtryck.</items>
                </term_definitions>
                <term_definitions code="at0005">
                    <items id="text">Hjärtfrekvens</items>
                    <items id="description">Intervall för uppmätt hjärtfrekvens.</items>
                </term_definitions>
                <term_definitions code="at0006">
                    <items id="text">Andningsfrekvens</items>
                    <items id="description">Intervall för uppmätt andningsfrekvens (AF).</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">Kroppstemperatur</items>
                    <items id="description">Intervall för uppmätt kroppstemperatur.</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">Medvetandegrad</items>
                    <items id="description">Kategorier för bedömning av medvetandegrad enligt AVPU-skalan.</items>
                </term_definitions>
                <term_definitions code="at0010">
                    <items id="text">&lt;=40 eller &gt;=131</items>
                    <items id="description">Hjärtfrekvens lägre än eller lika med 40 slag per minut, alternativt högre än eller lika med 131 slag per minut. Sätts till 3 poäng.</items>
                </term_definitions>
                <term_definitions code="at0011">
                    <items id="text">111-130</items>
                    <items id="description">Hjärtfrekvens mellan 111 och 130 slag per minut . Sätts till 2 poäng.</items>
                </term_definitions>
                <term_definitions code="at0012">
                    <items id="text">41-50 eller 91-110</items>
                    <items id="description">Hjärtfrekvens mellan 41 och 50 slag per minut, alternativt mellan 91 och 110 slag per minut. Sätts till 1 poäng.</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">51-90</items>
                    <items id="description">Hjärtfrekvens mellan 51 och 90 slag per minut. Sätts till 0 poäng.</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="text">111-219</items>
                    <items id="description">Systoliskt blodtryck mellan 111 och 219 mmHg. Sätts till 0 poäng.</items>
                </term_definitions>
                <term_definitions code="at0015">
                    <items id="text">101-110</items>
                    <items id="description">Systoliskt blodtryck mellan 101 och 110 mmHg. Sätts till 1 poäng.</items>
                </term_definitions>
                <term_definitions code="at0016">
                    <items id="text">91-100</items>
                    <items id="description">Systoliskt blodtryck mellan 91 och 100 mmHg. Sätts till 2 poäng.</items>
                </term_definitions>
                <term_definitions code="at0017">
                    <items id="text">&lt;=90 eller &gt;= 220</items>
                    <items id="description">Systoliskt blodtryck lägre än eller lika med 90 mmHg, alternativt högre än eller lika med 220 mmHg. Sätts till 3 poäng.</items>
                </term_definitions>
                <term_definitions code="at0018">
                    <items id="text">12-20</items>
                    <items id="description">Andningsfrekvens mellan 12 och 20 andetag per minut. Sätts till 0 poäng.</items>
                </term_definitions>
                <term_definitions code="at0019">
                    <items id="text">9-11</items>
                    <items id="description">Andningsfrekvens mellan 9 och 11 andetag per minut. Sätts till 1 poäng.</items>
                </term_definitions>
                <term_definitions code="at0020">
                    <items id="text">21-24</items>
                    <items id="description">Andningsfrekvens mellan 21 och 24 andetag per minut. Sätts till 2 poäng.</items>
                </term_definitions>
                <term_definitions code="at0021">
                    <items id="text">&lt;=8 eller &gt;=25</items>
                    <items id="description">Andningsfrekvens fler än eller lika med 8 andetag per minut, alternativt färre än eller lika med 25 andetag per minut. Sätts till 3 poäng.&#13;
</items>
                </term_definitions>
                <term_definitions code="at0022">
                    <items id="text">36,1-38,0</items>
                    <items id="description">Kroppstemperatur mellan 36,1 och 38,0 grader Celsius. Sätts till 0 poäng.</items>
                </term_definitions>
                <term_definitions code="at0023">
                    <items id="text">35,1-36,0 eller 38,1-39,0</items>
